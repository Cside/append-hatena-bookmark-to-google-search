{
    "rules": {
        "semicolon": [
            true,
            "never"
        ],
        "no-unused-expression": true,
        "no-trailing-whitespace": true,
        "align": [
            true,
            "elements",
            "members",
            "parameters",
            "statements"
        ],
        "no-consecutive-blank-lines": true,
        "no-boolean-literal-compare": true,
        "no-unnecessary-initializer": true,
        "no-unnecessary-qualifier": true,
        "one-line": [
            true,
            "check-catch",
            "check-finally",
            "check-else",
            "check-open-brace",
            "check-whitespace"
        ],
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ]
    }
}